<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core">
    <Color x:Key="MainBackground">#202020</Color>
    <Color x:Key="ModalBackground">#2C2C2C</Color>
    <Color x:Key="PopupMenuBackground">#2C2C2C</Color>
    <Color x:Key="TextAreaBackground">#2C2C2C</Color>
    <Color x:Key="ViewBackgroundColor">#111111</Color>
    <Color x:Key="MainTextColor">White</Color>

    <Color x:Key="MainContentColor">#F9F9F9</Color>
    <Color x:Key="MainContentSecondaryColor">#113E48</Color>
    <Color x:Key="NavbarMenuButtonColor">White</Color>

    <SolidColorBrush x:Key="BorderBrush" Opacity="0.06" Color="Black" />
    <SolidColorBrush x:Key="ContrastBorderBrush" Color="White" />
    <SolidColorBrush x:Key="ToggleSwitchBorderBrush" Color="White" Opacity="0.44" />

    <Brush x:Key="ModalBackgroundBrush" >#2C2C2C</Brush>
    <Brush x:Key="PreviewBackground">#202020</Brush>
    <Brush x:Key="SelectedTabBrush">#454341</Brush>
    <Brush x:Key="SelectedTabSecondaryBrush">#2C2C2C</Brush>
    <Brush x:Key="SelectedItemBrush">#3A3A3A</Brush>
    <Brush x:Key="MainBackgroundBrush">#202020</Brush>
    <Brush x:Key="MainContentSecondaryBrush">#113E48</Brush>
    <Brush x:Key="MainTextBrush">White</Brush>
    <Brush x:Key="ViewBackgroundBrush">#111111</Brush>
    <Brush x:Key="MainContentBrush">#F9F9F9</Brush>
    <Brush x:Key="LightGrayBrush">#999999</Brush>
    <Brush x:Key="DarkGrayBrush">#696969</Brush>
    <Brush x:Key="LightGray2Brush">#CACCD1</Brush>
    <Brush x:Key="LightGray3Brush">#EBEBEB</Brush>
    <Brush x:Key="LightGray4Brush">#C0C0C0</Brush>
    <Brush x:Key="GrayedOutItemBrush">#ABABAB</Brush>
    <Brush x:Key="GrayedOutBackgroundBrush">#202020</Brush>
    <dx:Theme x:Key="DXTheme">PeernetDarkTheme</dx:Theme>
</ResourceDictionary>