<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="MainBackground" >#EFF5F9</Color>
    <Color x:Key="ModalBackground" >#F3F3F3</Color>

    <Color x:Key="DarkGreen">#113E48</Color>
    <Color x:Key="LightGray">#999999</Color>
    <Color x:Key="LightGray2">#CACCD1</Color>
    <Color x:Key="GrayedOutItemColor">#ABABAB</Color>

    <Brush x:Key="MainBackgroundBrush">#EFF5F9</Brush>
    <Brush x:Key="DarkGreenBrush">#113E48</Brush>
    <Brush x:Key="LightGrayBrush">#999999</Brush>
    <Brush x:Key="LightGray2Brush">#CACCD1</Brush>
    <Brush x:Key="LightGray3Brush">#EBEBEB</Brush>
    <Brush x:Key="LightGray4Brush">#C0C0C0</Brush>
    <Brush x:Key="GrayedOutItemBrush">#ABABAB</Brush>
    <Brush x:Key="GrayedOutBackgroundBrush">#C7C7C7</Brush>
</ResourceDictionary>