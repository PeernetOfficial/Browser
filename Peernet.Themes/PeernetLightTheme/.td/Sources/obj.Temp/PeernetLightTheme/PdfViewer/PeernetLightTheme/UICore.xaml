        <dxt:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.PeernetLightTheme.v22.1;component/PdfViewer/PeernetLightTheme/UICustom.xaml" />
        <dxt:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.PeernetLightTheme.v22.1;component/PdfViewer/PeernetLightTheme/UIPreset.xaml" />
    </ResourceDictionary.MergedDictionaries>

    

    <SolidColorBrush x:Key="{dxpdft:PdfViewerThemeKey ResourceKey=PdfPresenterControlBackgroundBrush}" Color="#FFFFFFFF" x:Uid="@Color=Window.Background" />

    

    <Color x:Key="{dxpdft:PdfViewerThemeKey ResourceKey=HighlightSearchTextColor}" x:Uid="@Content_Property=SelectionBackground">#FFDEECF9</Color>

    <SolidColorBrush x:Key="{dxpdft:PdfViewerThemeKey ResourceKey=HighlightSelectionColor}" Color="#FFDEECF9" Opacity="0.38" x:Uid="@Color=SelectionBackground" />

    

    <Color x:Key="{dxpdft:PdfViewerThemeKey ResourceKey=CaretColor}" x:Uid="@Content_Property=Backstage.Window.Background">#FF106EBE</Color>

    

    <Color x:Key="{dxpdft:PdfViewerThemeKey ResourceKey=StartScreenCaptionForegroundColor}" x:Uid="@Content_Property=Foreground">#FF444444</Color>

    <Color x:Key="{dxpdft:PdfViewerThemeKey ResourceKey=StartScreenOpenDocumentForegroundColor}" x:Uid="@Content_Property=Foreground">#FF444444</Color>

    <Color x:Key="{dxpdft:PdfViewerThemeKey ResourceKey=StartScreenDocumentLinkForegroundColor}" x:Uid="@Content_Property=Custom.Blue">#FF0026C1</Color>

    <Color x:Key="{dxpdft:PdfViewerThemeKey ResourceKey=LoadingDocumentLinkForegroundColor}" x:Uid="@Content_Property=Custom.Blue">#FF0026C1</Color>

    <SolidColorBrush x:Key="{dxpdft:PdfViewerThemeKey ResourceKey=PdfCommentViewerSelectionBrush}" Color="#FFEFF6FC" x:Uid="@Color=HoverBackground" />

    <SolidColorBrush x:Key="{dxpdft:PdfViewerThemeKey ResourceKey=PdfCommentViewerCommentDateForegroundBrush}" Color="#FF444444" Opacity="0.56" x:Uid="@Color=Foreground" />

