<ResourceDictionary mc:Ignorable="cs" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:cs="colorscheme_ignorable_uri" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal" xmlns:dxsps="http://schemas.devexpress.com/winfx/2008/xaml/spreadsheet" xmlns:dxspsi="http://schemas.devexpress.com/winfx/2008/xaml/spreadsheet/internal" xmlns:dxspst="http://schemas.devexpress.com/winfx/2008/xaml/spreadsheet/themekeys" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxcore_unique_xmlns="http://schemas.devexpress.com/winfx/2008/xaml/core">

    <ResourceDictionary.MergedDictionaries>
        <dxt:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.PeernetLightTheme.v21.2;component/Spreadsheet/PeernetLightTheme/UICore.xaml" />
    </ResourceDictionary.MergedDictionaries>

    

    <ControlTemplate x:Key="{dxspst:SpreadsheetThemeKey ResourceKey=HeadersControlTemplate}" TargetType="{x:Type dxspsi:WorksheetHeadersControl}">
        <dxspsi:HeadersPanel LayoutInfo="{Binding Path=(dxspsi:SpreadsheetViewControl.ActiveSpreadsheetView).LayoutInfo, RelativeSource={RelativeSource Self}}" Orientation="{TemplateBinding Orientation}" />
    </ControlTemplate>

    

    <ControlTemplate x:Key="{dxspst:SpreadsheetThemeKey ResourceKey=HorizontalHeaderItem}" TargetType="{x:Type dxspsi:HeaderItem}">
        <dx:DXBorder x:Name="IsDefault" Background="#1D4E27A6" BorderBrush="#1D7F27A7" BorderThickness="0,0,1,1" cs:id="Border_0001" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Spreadsheet\Office2019White\Spreadsheet\SpreadsheetHeaderItem_Base.xaml;27;10">
            <TextBlock x:Name="Content" FontSize="{TemplateBinding FontHeight}" Foreground="#1D5127A8" HorizontalAlignment="Center" Text="{TemplateBinding Text}" TextAlignment="Center" VerticalAlignment="Center" cs:id="TextBlock_0000" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Spreadsheet\Office2019White\Spreadsheet\SpreadsheetHeaderItem_Base.xaml;32;14" />
        </dx:DXBorder>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Foreground" TargetName="Content" Value="#1D6027A9" cs:id="Setter_0004" />
                <Setter Property="Background" TargetName="IsDefault" Value="#1D3327AA" cs:id="Setter_0000" />
                <Setter Property="BorderBrush" TargetName="IsDefault" Value="#1D0227AB" cs:id="Setter_0002" />
            </Trigger>
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="Foreground" TargetName="Content" Value="#1D9527AC" cs:id="Setter_0005" />
                <Setter Property="Background" TargetName="IsDefault" Value="#1DA427AD" cs:id="Setter_0001" />
                <Setter Property="BorderBrush" TargetName="IsDefault" Value="#1DF727AE" cs:id="Setter_0003" />
            </Trigger>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="MinHeight" Value="44" />
                <Setter Property="MinWidth" Value="44" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    

    <ControlTemplate x:Key="{dxspst:SpreadsheetThemeKey ResourceKey=VerticalHeaderItem}" TargetType="{x:Type dxspsi:HeaderItem}">
        <dx:DXBorder x:Name="IsDefault" Background="#1DC627AF" BorderBrush="#1DAB27B0" BorderThickness="0,0,1,1" cs:id="Border_0001" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Spreadsheet\Office2019White\Spreadsheet\SpreadsheetHeaderItem_Base.xaml;80;10">
            <TextBlock x:Name="Content" FontSize="{TemplateBinding FontHeight}" Foreground="#1D9A27B1" HorizontalAlignment="Center" Text="{TemplateBinding Text}" TextAlignment="Center" VerticalAlignment="Center" cs:id="TextBlock_0000" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Spreadsheet\Office2019White\Spreadsheet\SpreadsheetHeaderItem_Base.xaml;85;14" />
        </dx:DXBorder>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Foreground" TargetName="Content" Value="#1DC927B2" cs:id="Setter_0004" />
                <Setter Property="Background" TargetName="IsDefault" Value="#1DF827B3" cs:id="Setter_0000" />
                <Setter Property="BorderBrush" TargetName="IsDefault" Value="#1D6F27B4" cs:id="Setter_0002" />
            </Trigger>
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="Foreground" TargetName="Content" Value="#1D5E27B5" cs:id="Setter_0005" />
                <Setter Property="Background" TargetName="IsDefault" Value="#1D0D27B6" cs:id="Setter_0001" />
                <Setter Property="BorderBrush" TargetName="IsDefault" Value="#1D3C27B7" cs:id="Setter_0003" />
            </Trigger>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="MinHeight" Value="44" />
                <Setter Property="MinWidth" Value="44" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    

    <Style x:Key="{dxspst:SpreadsheetThemeKey ResourceKey=TopLeftCornerStyle}" TargetType="{x:Type Border}">
        <Setter Property="dxcore_unique_xmlns:BlendHelper2.StyleInfo" Value="Spreadsheet\Office2019White\Spreadsheet\SpreadsheetHeaderItem_Base.xaml;132;6" /><Setter Property="Background" Value="#1D1227B8" cs:id="Setter_0000" />
        <Setter Property="BorderBrush" Value="#1D2327B9" cs:id="Setter_0001" />
        <Setter Property="BorderThickness" Value="0,0,1,1" />
        <Style.Triggers>
            <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                <Setter Property="MinHeight" Value="44" />
                <Setter Property="MinWidth" Value="44" />
            </Trigger>
        </Style.Triggers>
    </Style>

</ResourceDictionary>