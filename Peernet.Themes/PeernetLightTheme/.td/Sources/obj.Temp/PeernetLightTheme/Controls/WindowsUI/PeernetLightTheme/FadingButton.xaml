                    <Path x:Name="Down" Fill="#1DE40978" Height="8" Opacity="0" Style="{StaticResource UIShape.Down}" Width="16" cs:id="Path_0003" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Controls\WindowsUI\Office2019White\FadingButton.xaml;58;22" />
