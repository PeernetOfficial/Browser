    <Style x:Key="{dxrt:RibbonPageThemeKey ResourceKey=RightRepeatButtonStyle}" TargetType="{x:Type RepeatButton}">
        <Setter Property="dxcore_unique_xmlns:BlendHelper2.StyleInfo" Value="Ribbon\Office2019White\RibbonPage\RibbonPageRepeatButtons.xaml;96;6" /><Setter Property="Margin" Value="0,0,0,1" />
