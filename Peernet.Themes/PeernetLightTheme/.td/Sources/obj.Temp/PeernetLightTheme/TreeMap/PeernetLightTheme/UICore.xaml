    <SolidColorBrush x:Key="{dxtmt:TreeMapBrushesThemeKey ResourceKey=GroupBackground}" Color="#FFFFFFFF" Opacity="0.62" x:Uid="@Color=Control.Background" />

    <SolidColorBrush x:Key="{dxtmt:TreeMapBrushesThemeKey ResourceKey=GroupForeground}" Color="#FF106EBE" x:Uid="@Color=Focused" />

    

    <SolidColorBrush x:Key="{dxtmt:TreeMapBrushesThemeKey ResourceKey=SubGroupBackground}" Color="#FFFFFFFF" x:Uid="@Color=Editor.Background" />

    <SolidColorBrush x:Key="{dxtmt:TreeMapBrushesThemeKey ResourceKey=SubGroupForeground}" Color="#FF444444" x:Uid="@Color=Foreground" />

    

    <SolidColorBrush x:Key="{dxtmt:TreeMapBrushesThemeKey ResourceKey=ToolTipBackground}" Color="#FF106EBE" x:Uid="@Color=Backstage.Window.Background" />

    <SolidColorBrush x:Key="{dxtmt:TreeMapBrushesThemeKey ResourceKey=ToolTipForeground}" Color="#FFFFFFFF" x:Uid="@Color=Backstage.Foreground" />

