        <dx:DXBorder BorderBrush="#FFE1DFDD" BorderThickness="0,1,0,0" HorizontalAlignment="Right" Margin="0,-1,0,0" VerticalAlignment="Top" cs:id="Border_0001" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Scheduler\Office2019White\DayView\TimeRuler.xaml;25;10" x:Uid="@BorderBrush=Delimiter">
