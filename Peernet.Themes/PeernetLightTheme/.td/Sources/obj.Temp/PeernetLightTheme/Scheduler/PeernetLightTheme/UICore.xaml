        <dxt:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.PeernetLightTheme.v21.2;component/Scheduler/PeernetLightTheme/UICustom.xaml" />
        <dxt:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.PeernetLightTheme.v21.2;component/Scheduler/PeernetLightTheme/UIPreset.xaml" />
    </ResourceDictionary.MergedDictionaries>

    

    <SolidColorBrush x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=HeaderForegroundColor}" Color="#1D942798" />

    <SolidColorBrush x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=AppointmentForegroundColor}" Color="#1DA52799" />

    

    <SolidColorBrush x:Key="{dxscht:DayViewThemeKey ResourceKey=AllDayAreaBottomBorderBackground}" Color="#1DF6279A" />

    

    <dxschi:ColorCollection x:Key="{dxscht:Office2007ThemeKey ResourceKey=HeaderBaseColors}">
        <dxschi:ColorDefinition Name="ColorDefinition_HeaderBaseBorder" Value="#1DC7279B" cs:id="ColorDefinition_0000" />
        <dxschi:ColorDefinition Name="ColorDefinition_HeaderBaseBackground" Value="#1D50279C" cs:id="ColorDefinition_0001" />
        <dxschi:ColorDefinition Name="ColorDefinition_AltHeaderBaseBackground" Value="#1D61279D" cs:id="ColorDefinition_0002" />
    </dxschi:ColorCollection>

</ResourceDictionary>