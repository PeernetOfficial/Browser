                                    </Grid.ColumnDefinitions>
                                    <dxpgp:PivotPrintHeaderArea Area="DataArea" Grid.Column="0" Grid.Row="0" HasBottomBorder="{Binding ShowDataAreaHeadersBottomBorder, Mode=OneTime}" MaxWidth="{Binding Path=RowValuesWidth, Mode=OneTime}" PivotGrid="{Binding PivotGrid, Mode=OneTime}" VerticalAlignment="Top" Visibility="{Binding DataHeadersVisible, Converter={StaticResource boolToVisibilityConverter}, Mode=OneTime}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="PivotGrid\Generic\PrintingTemplates.xaml;47;38" />
                                    <dxpgp:PivotPrintHeaderArea Area="RowArea" Grid.Column="0" Grid.Row="1" HasBottomBorder="False" HasTopBorder="{Binding ShowRowAreaHeadersTopBorder, Mode=OneTime}" HorizontalAlignment="Left" HorizontalContentAlignment="Left" MaxWidth="{Binding Path=RowValuesWidth, Mode=OneTime}" PivotGrid="{Binding PivotGrid, Mode=OneTime}" VerticalAlignment="Bottom" Visibility="{Binding RowHeadersVisible, Converter={StaticResource boolToVisibilityConverter}, Mode=OneTime}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="PivotGrid\Generic\PrintingTemplates.xaml;55;38" />
                                    <dxpgp:PivotPrintFieldValues x:Name="rowFieldValues" dxpg:PivotGridControl.PivotGrid="{Binding PivotGrid, Mode=OneTime}" Grid.Column="0" Grid.Row="2" HorizontalAlignment="Left" IsChild="True" IsColumn="False" PrintItems="{Binding RowItems, Mode=OneTime}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="PivotGrid\Generic\PrintingTemplates.xaml;66;38" />
