    <DataTemplate x:Key="{dxpgi:PrintingThemeKey ResourceKey=FieldCellKpiTemplate}">
        <dxpgp:ImageExporter dxp:ExportSettings.BorderColor="#808080" dxp:ExportSettings.TargetType="Panel" ExportSettings="{Binding Mode=OneTime}" cs:id="ImageExporter_0000">
            <dxpgp:ImageExporter.Template>
                <ControlTemplate TargetType="{x:Type dxpgp:ImageExporter}">
                    <Image dxp:ExportSettings.TargetType="Image" MaxHeight="15" MaxWidth="15" Source="{TemplateBinding ImageSource}" Stretch="None" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="PivotGrid\Generic\PrintingTemplates.xaml;198;22" />
                </ControlTemplate>
            </dxpgp:ImageExporter.Template>
        </dxpgp:ImageExporter>
    </DataTemplate>

    <DataTemplate x:Key="{dxpgi:PrintingThemeKey ResourceKey=FieldValueTemplate}">
