                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>
                                        <dxpgp:PivotPrintHeaderArea Area="ColumnArea" HasBottomBorder="{Binding ShowColumnAreaHeadersBottomBorder, Mode=OneTime}" PivotGrid="{Binding PivotGrid, Mode=OneTime}" VerticalAlignment="Bottom" Visibility="{Binding ColumnHeadersVisible, Converter={StaticResource boolToVisibilityConverter}, Mode=OneTime}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="PivotGrid\Generic\PrintingTemplates.xaml;82;42" />
                                        <dxpgp:PivotPrintFieldValues x:Name="columnFieldValues" dxpg:PivotGridControl.PivotGrid="{Binding PivotGrid, Mode=OneTime}" Grid.Row="1" HasTopBorder="{Binding ShowColumnAreaValuesTopBorder, Mode=OneTime}" IsChild="True" IsColumn="True" PrintItems="{Binding ColumnItems, Mode=OneTime}" VerticalAlignment="Bottom" Visibility="{Binding ShowColumnFieldValues, Mode=OneTime}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="PivotGrid\Generic\PrintingTemplates.xaml;87;42" />
                                    </Grid>
