    <SolidColorBrush x:Key="{dxt:BadgesThemeKey ErrorBackground}" Color="#FFEF3C3B" />

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey ErrorBorderBrush}" Color="#FFEF3C3B" />

