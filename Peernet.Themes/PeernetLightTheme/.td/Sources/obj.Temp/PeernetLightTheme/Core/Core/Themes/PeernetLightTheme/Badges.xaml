    <SolidColorBrush x:Key="{dxt:BadgesThemeKey ErrorBackground}" Color="#1DC70833" />

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey ErrorBorderBrush}" Color="#1D500834" />

    <Thickness x:Key="{dxt:BadgesThemeKey ErrorBorderThickness}">0</Thickness>

