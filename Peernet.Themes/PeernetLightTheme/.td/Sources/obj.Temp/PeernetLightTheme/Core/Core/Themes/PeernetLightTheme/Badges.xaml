    <SolidColorBrush x:Key="{dxt:BadgesThemeKey InformationForeground}" Color="#1D7E083B" />

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey InformationBackground}" Color="#1DE9083C" />

    <SolidColorBrush x:Key="{dxt:BadgesThemeKey InformationBorderBrush}" Color="#1DD8083D" />

    <Thickness x:Key="{dxt:BadgesThemeKey InformationBorderThickness}">0</Thickness>

    <Thickness x:Key="{dxt:BadgesThemeKey Padding}">3,3,3,4</Thickness>

    <Thickness x:Key="{dxt:BadgesThemeKey TouchPadding}">5,5,5,6</Thickness>

