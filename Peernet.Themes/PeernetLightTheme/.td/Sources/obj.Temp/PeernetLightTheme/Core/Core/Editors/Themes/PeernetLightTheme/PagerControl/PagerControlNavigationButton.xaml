    <ControlTemplate x:Key="{dxet:PagerControlThemeKey ResourceKey=NavigationButtonTemplate}" TargetType="dxe:PagerControlNavigationButton">
        <Grid x:Name="Root" Background="#1D23060E" cs:id="Grid_0000" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Core\Core\Editors\Themes\Office2019White\PagerControl\PagerControlNavigationButton.xaml;20;10">
            <dx:DXBorder Name="IsDefault" Background="#1D12060F" BorderBrush="#1D7F0610" BorderThickness="1" cs:id="Border_0000" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Core\Core\Editors\Themes\Office2019White\PagerControl\PagerControlNavigationButton.xaml;23;14" />
            <ContentPresenter x:Name="Content" Margin="{TemplateBinding Padding}" TextElement.Foreground="#1D4E0611" cs:id="ContentPresenter_0000" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Core\Core\Editors\Themes\Office2019White\PagerControl\PagerControlNavigationButton.xaml;28;14" />
