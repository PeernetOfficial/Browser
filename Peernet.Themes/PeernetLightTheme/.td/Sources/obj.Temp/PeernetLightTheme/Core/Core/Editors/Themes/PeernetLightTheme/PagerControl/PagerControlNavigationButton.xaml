                <Setter Property="TextElement.Foreground" TargetName="Content" Value="#1D1D0612" cs:id="Setter_0003" />
                <Setter Property="Background" TargetName="IsDefault" Value="#1D2C0613" cs:id="Setter_0000" />
                <Setter Property="BorderBrush" TargetName="IsDefault" Value="#1DBB0614" cs:id="Setter_0002" />
