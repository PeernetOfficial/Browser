    <ControlTemplate x:Key="{dxct:ChartControlThemeKey ResourceKey=AxisScrollBarTemplate}" TargetType="dxc:AxisScrollBar">
        <Grid x:Name="Root" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Charts\Office2019White\AxisScrollBar.xaml;414;10">
            <Grid x:Name="PART_HorizontalRoot" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Charts\Office2019White\AxisScrollBar.xaml;415;14">
                <dxc:ScrollBarBorder x:Name="PART_HorizontalBorder" Annotations="{TemplateBinding Annotations}" Background="#1DCB01FA" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" cs:id="Border_0000" />
                <Grid x:Name="PART_HorizontalContainer" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Charts\Office2019White\AxisScrollBar.xaml;422;18">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>
                    <dxc:ScrollBarRepeatButton x:Name="PART_HorizontalSmallDecrease" AutomationProperties.Name="HorizontalScrollbarSmallDecrement" Grid.Column="0" Interval="50" IsTabStop="False" Margin="1,1,-1,1" Template="{StaticResource HorizontalDecrementTemplate}" Width="22" />
                    <dxc:ScrollBarRepeatButton x:Name="PART_HorizontalLargeDecrease" AutomationProperties.Name="HorizontalScrollbarLargeDecrement" Grid.Column="1" Interval="50" IsTabStop="False" Template="{StaticResource RepeatButtonTemplate}" />
                    <dxc:AxisScrollBarThumb x:Name="PART_HorizontalThumb" Grid.Column="2" Margin="0,1,0,1" MinWidth="22" Template="{StaticResource HorizontalThumbTemplate}" />
                    <dxc:AxisScrollBarThumbResizer x:Name="PART_HorizontalThumbDecrease" Cursor="SizeWE" Grid.Column="2" HorizontalAlignment="Left" MinWidth="4" Template="{StaticResource HorizontalThumbResizerTemplate}" />
                    <dxc:AxisScrollBarThumbResizer x:Name="PART_HorizontalThumbIncrease" Cursor="SizeWE" Grid.Column="2" HorizontalAlignment="Right" MinWidth="4" Template="{StaticResource HorizontalThumbResizerTemplate}" />
                    <dxc:ScrollBarRepeatButton x:Name="PART_HorizontalLargeIncrease" AutomationProperties.Name="HorizontalScrollbarLargeIncrement" Grid.Column="3" Interval="50" IsTabStop="False" Template="{StaticResource RepeatButtonTemplate}" />
                    <dxc:ScrollBarRepeatButton x:Name="PART_HorizontalSmallIncrease" AutomationProperties.Name="HorizontalScrollbarSmallIncrement" Grid.Column="4" Interval="50" IsTabStop="False" Margin="-1,1,1,1" Template="{StaticResource HorizontalIncrementTemplate}" Width="22" />
                </Grid>
            </Grid>
            <Grid x:Name="PART_VerticalRoot" Visibility="Collapsed" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Charts\Office2019White\AxisScrollBar.xaml;477;14">
                <dxc:ScrollBarBorder x:Name="PART_VerticalBorder" Annotations="{TemplateBinding Annotations}" Background="#1DFA01FB" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" cs:id="Border_0001" />
                <Grid x:Name="PART_VerticalContainer" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Charts\Office2019White\AxisScrollBar.xaml;484;18">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <dxc:ScrollBarRepeatButton x:Name="PART_VerticalSmallIncrease" AutomationProperties.Name="VerticalScrollbarSmallIncrement" Grid.Row="0" Height="22" Interval="50" IsTabStop="False" Margin="1,1,1,-1" Template="{StaticResource VerticalIncrementTemplate}" />
                    <dxc:ScrollBarRepeatButton x:Name="PART_VerticalLargeIncrease" AutomationProperties.Name="VerticalScrollbarLargeIncrement" Grid.Row="1" Interval="50" IsTabStop="False" Margin="0,0,1,0" Template="{StaticResource RepeatButtonTemplate}" />
                    <dxc:AxisScrollBarThumb x:Name="PART_VerticalThumb" Grid.Row="2" Margin="1,0,1,0" MinHeight="22" Template="{StaticResource VerticalThumbTemplate}" />
                    <dxc:AxisScrollBarThumbResizer x:Name="PART_VerticalThumbIncrease" Cursor="SizeNS" Grid.Row="2" MinHeight="4" Template="{StaticResource VerticalThumbResizerTemplate}" VerticalAlignment="Top" />
                    <dxc:AxisScrollBarThumbResizer x:Name="PART_VerticalThumbDecrease" Cursor="SizeNS" Grid.Row="2" MinHeight="4" Template="{StaticResource VerticalThumbResizerTemplate}" VerticalAlignment="Bottom" />
                    <dxc:ScrollBarRepeatButton x:Name="PART_VerticalLargeDecrease" AutomationProperties.Name="VerticalScrollbarLargeDecrement" Grid.Row="3" Interval="50" IsTabStop="False" Margin="0,0,1,0" Template="{StaticResource RepeatButtonTemplate}" />
                    <dxc:ScrollBarRepeatButton x:Name="PART_VerticalSmallDecrease" AutomationProperties.Name="VerticalScrollbarSmallDecrement" Grid.Row="4" Height="22" Interval="50" IsTabStop="False" Margin="1,-1,1,1" Template="{StaticResource VerticalDecrementTemplate}" />
                </Grid>
            </Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal" />
                    <VisualState x:Name="MouseOver" />
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="Root" Storyboard.TargetProperty="Opacity" To="0.5" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>

