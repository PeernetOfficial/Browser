    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Charts.SeriesLabelPresentation}" BasedOn="{StaticResource baseSeriesLabelStyle}" TargetType="{x:Type local:SeriesLabelPresentation}"><Setter Property="dxcore_unique_xmlns:BlendHelper2.StyleInfo" Value="Charts\Generic\SeriesLabel.xaml;63;6" /></Style>

