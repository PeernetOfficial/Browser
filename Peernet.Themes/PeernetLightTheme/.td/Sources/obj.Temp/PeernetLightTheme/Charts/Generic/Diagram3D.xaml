                                    <ItemsPanelTemplate>
                                        <local:AnnotationLayoutPanel />
                                    </ItemsPanelTemplate>
                                </local:ChartItemsControl.ItemsPanel>
                            </local:ChartItemsControl>
                            <local:SelectionControl IsHighlighted="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectionInfo.IsHighlighted}" IsSelected="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectionInfo.IsSelected}" />
                        </Grid>
                    </local:ChartContentControl>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Charts.SimpleDiagram3D}" BasedOn="{StaticResource Diagram3DStyle}" TargetType="{x:Type local:SimpleDiagram3D}">
        <Setter Property="dxcore_unique_xmlns:BlendHelper2.StyleInfo" Value="Charts\Generic\Diagram3D.xaml;55;6" /><Setter Property="local:XYDiagram3D.Material" Value="{StaticResource DefaultXYDiagramMaterial}" />
        <Setter Property="Annotations" Value="{Binding Path=(local:ChartControlBase.Chart).DiagramAnnotations, RelativeSource={RelativeSource Self}}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:SimpleDiagram3D}">
                    <local:ChartContentControl x:Name="SimpleDiagram3DWrapper" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Padding="{TemplateBinding Padding}" Template="{DynamicResource {dxt:ChartControlThemeKey ResourceKey=SimpleDiagram3DWrapperTemplate}}" Visibility="{TemplateBinding Visibility}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Charts\Generic\Diagram3D.xaml;63;22">
                        <Grid dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Charts\Generic\Diagram3D.xaml;70;26">
                            <local:SimpleDiagramItemsControl DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}}" ItemsSource="{TemplateBinding Series}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Charts\Generic\Diagram3D.xaml;71;30">
                                <local:SimpleDiagramItemsControl.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <local:SimpleDiagramPanel SimpleDiagram="{Binding}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Charts\Generic\Diagram3D.xaml;74;42" />
                                    </ItemsPanelTemplate>
                                </local:SimpleDiagramItemsControl.ItemsPanel>
                                <local:SimpleDiagramItemsControl.ItemTemplate>
                                    <DataTemplate>
                                        <local:TitlesLayoutControl Titles="{Binding Path=Content.Titles, RelativeSource={RelativeSource TemplatedParent}}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Charts\Generic\Diagram3D.xaml;79;42">
                                            <local:TitlesLayoutControl.MasterElement>
                                                <local:VisualContainer Diagram="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type local:Diagram}}}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Charts\Generic\Diagram3D.xaml;81;50" />
                                            </local:TitlesLayoutControl.MasterElement>
                                        </local:TitlesLayoutControl>
                                    </DataTemplate>
                                </local:SimpleDiagramItemsControl.ItemTemplate>
                            </local:SimpleDiagramItemsControl>
                            <local:ChartItemsControl ItemsSource="{Binding Annotations, RelativeSource={RelativeSource TemplatedParent}}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Charts\Generic\Diagram3D.xaml;87;30">
                                <local:ChartItemsControl.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <local:AnnotationLayoutPanel />
                                    </ItemsPanelTemplate>
                                </local:ChartItemsControl.ItemsPanel>
                            </local:ChartItemsControl>
                            <local:SelectionControl IsHighlighted="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectionInfo.IsHighlighted}" IsSelected="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectionInfo.IsSelected}" />
                        </Grid>
                    </local:ChartContentControl>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>