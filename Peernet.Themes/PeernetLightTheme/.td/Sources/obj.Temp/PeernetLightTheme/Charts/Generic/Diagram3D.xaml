        <Setter Property="dxcore_unique_xmlns:BlendHelper2.StyleInfo" Value="Charts\Generic\Diagram3D.xaml;23;6" /><Setter Property="DomainBrush" Value="{DynamicResource {dxt:ChartBrushesThemeKey ResourceKey=Domain3DBrush}}" />
        <Setter Property="Annotations" Value="{Binding Path=(local:ChartControlBase.Chart).DiagramAnnotations, RelativeSource={RelativeSource Self}}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:XYDiagram3D">
                    <local:ChartContentControl x:Name="XYDiagram3DWrapper" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Padding="{TemplateBinding Padding}" Template="{DynamicResource {dxt:ChartControlThemeKey ResourceKey=XYDiagram3DWrapperTemplate}}" Visibility="{TemplateBinding Visibility}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Charts\Generic\Diagram3D.xaml;31;22">
                        <Grid dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Charts\Generic\Diagram3D.xaml;38;26">
                            <local:VisualContainer Diagram="{Binding RelativeSource={RelativeSource TemplatedParent}}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Charts\Generic\Diagram3D.xaml;39;30" />
                            <local:ChartItemsControl ItemsSource="{Binding Annotations, RelativeSource={RelativeSource TemplatedParent}}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Charts\Generic\Diagram3D.xaml;40;30">
