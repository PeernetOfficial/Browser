<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxsch="http://schemas.devexpress.com/winfx/2008/xaml/scheduler"
                    xmlns:dxschcore="clr-namespace:DevExpress.XtraScheduler;assembly=DevExpress.XtraScheduler.v22.2.Core"
                    xmlns:dxschi="http://schemas.devexpress.com/winfx/2008/xaml/scheduler/internal"
                    xmlns:dxscht="http://schemas.devexpress.com/winfx/2008/xaml/scheduler/themekeys"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <dxschi:HorizontalAppointmentStyleSelector x:Key="{dxscht:StyleSelectorThemeKey ResourceKey=HorizontalAppointmentStyle}">
        <dxschi:HorizontalAppointmentStyleSelector.Properties>
            <dxschi:HorizontalAppointmentStyleSelectorProperties HorizontalAppointmentLongerThanADay="{DynamicResource {dxscht:SchedulerViewThemeKey ResourceKey=HorizontalAppointmentLongerThanADayStyle}}" HorizontalAppointmentSameDay="{DynamicResource {dxscht:SchedulerViewThemeKey ResourceKey=HorizontalAppointmentSameDayStyle}}"/>
        </dxschi:HorizontalAppointmentStyleSelector.Properties>
    </dxschi:HorizontalAppointmentStyleSelector>

    <dxschi:AppointmentStyleSelector x:Key="{dxscht:StyleSelectorThemeKey ResourceKey=VerticalAppointmentStyle}">
        <dxschi:AppointmentStyleSelector.Properties>
            <dxschi:AppointmentStyleSelectorProperties AppointmentStyle="{DynamicResource {dxscht:SchedulerViewThemeKey ResourceKey=VerticalAppointmentStyle}}"/>
        </dxschi:AppointmentStyleSelector.Properties>
    </dxschi:AppointmentStyleSelector>

    <dxschi:DayViewContentStyleSelector x:Key="{dxscht:DayViewThemeKey ResourceKey=ContentStyleSelector}">
        <dxschi:DayViewContentStyleSelector.Properties>
            <!--BEGIN WPF-->
            <dxschi:ViewContentStyleSelectorProperties GroupByDate="{dxscht:DayViewThemeKey ResourceKey=DayViewGroupByDate}"
                                                       GroupByNone="{dxscht:DayViewThemeKey ResourceKey=DayViewGroupByNone}"
                                                       GroupByResource="{dxscht:DayViewThemeKey ResourceKey=DayViewGroupByResource}"/>
            <!--END WPF-->
            <!--ONLY SL
            <dxschi:ViewContentStyleSelectorProperties GroupByNone="{StaticResource DayViewThemeKey_DayViewGroupByNone}"
                    GroupByDate="{StaticResource DayViewThemeKey_DayViewGroupByDate}"
                    GroupByResource="{StaticResource DayViewThemeKey_DayViewGroupByResource}"
                />
            ONLY SL-->
        </dxschi:DayViewContentStyleSelector.Properties>
    </dxschi:DayViewContentStyleSelector>

    <dxschi:MonthViewContentStyleSelector x:Key="{dxscht:MonthViewThemeKey ResourceKey=ContentStyleSelector}">
        <dxschi:MonthViewContentStyleSelector.Properties>
            <!--BEGIN WPF-->
            <dxschi:ViewContentStyleSelectorProperties GroupByDate="{dxscht:MonthViewThemeKey ResourceKey=MonthViewGroupByDate}"
                                                       GroupByNone="{dxscht:MonthViewThemeKey ResourceKey=MonthViewGroupByNone}"
                                                       GroupByResource="{dxscht:MonthViewThemeKey ResourceKey=MonthViewGroupByResource}"/>
            <!--END WPF-->
            <!--ONLY SL
            <dxschi:ViewContentStyleSelectorProperties GroupByNone="{StaticResource MonthViewThemeKey_MonthViewGroupByNone}"
                GroupByDate="{StaticResource MonthViewThemeKey_MonthViewGroupByDate}"
                GroupByResource="{StaticResource MonthViewThemeKey_MonthViewGroupByResource}" />
            ONLY SL-->
        </dxschi:MonthViewContentStyleSelector.Properties>
    </dxschi:MonthViewContentStyleSelector>

    <dxschi:WorkWeekViewContentStyleSelector x:Key="{dxscht:WorkWeekViewThemeKey ResourceKey=ContentStyleSelector}">
        <dxschi:WorkWeekViewContentStyleSelector.Properties>
            <!--BEGIN WPF-->
            <dxschi:ViewContentStyleSelectorProperties GroupByDate="{dxscht:DayViewThemeKey ResourceKey=DayViewGroupByDate}"
                                                       GroupByNone="{dxscht:DayViewThemeKey ResourceKey=DayViewGroupByNone}"
                                                       GroupByResource="{dxscht:DayViewThemeKey ResourceKey=DayViewGroupByResource}"/>
            <!--END WPF-->
            <!--ONLY SL
            <dxschi:ViewContentStyleSelectorProperties GroupByNone="{StaticResource DayViewThemeKey_DayViewGroupByNone}"
                GroupByDate="{StaticResource DayViewThemeKey_DayViewGroupByDate}"
                GroupByResource="{StaticResource DayViewThemeKey_DayViewGroupByResource}" />
            ONLY SL-->
        </dxschi:WorkWeekViewContentStyleSelector.Properties>
    </dxschi:WorkWeekViewContentStyleSelector>

    <dxschi:FullWeekViewContentStyleSelector x:Key="{dxscht:FullWeekViewThemeKey ResourceKey=ContentStyleSelector}">
        <dxschi:FullWeekViewContentStyleSelector.Properties>
            <!--BEGIN WPF-->
            <dxschi:ViewContentStyleSelectorProperties GroupByDate="{dxscht:DayViewThemeKey ResourceKey=DayViewGroupByDate}"
                                                       GroupByNone="{dxscht:DayViewThemeKey ResourceKey=DayViewGroupByNone}"
                                                       GroupByResource="{dxscht:DayViewThemeKey ResourceKey=DayViewGroupByResource}"/>
            <!--END WPF-->
            <!--ONLY SL
            <dxschi:ViewContentStyleSelectorProperties GroupByNone="{StaticResource DayViewThemeKey_DayViewGroupByNone}"
                GroupByDate="{StaticResource DayViewThemeKey_DayViewGroupByDate}"
                GroupByResource="{StaticResource DayViewThemeKey_DayViewGroupByResource}" />
            ONLY SL-->
        </dxschi:FullWeekViewContentStyleSelector.Properties>
    </dxschi:FullWeekViewContentStyleSelector>

    <dxschi:WeekViewContentStyleSelector x:Key="{dxscht:WeekViewThemeKey ResourceKey=ContentStyleSelector}">
        <dxschi:WeekViewContentStyleSelector.Properties>
            <!--BEGIN WPF-->
            <dxschi:ViewContentStyleSelectorProperties GroupByDate="{dxscht:WeekViewThemeKey ResourceKey=WeekViewGroupByDate}"
                                                       GroupByNone="{dxscht:WeekViewThemeKey ResourceKey=WeekViewGroupByNone}"
                                                       GroupByResource="{dxscht:WeekViewThemeKey ResourceKey=WeekViewGroupByResource}"/>
            <!--END WPF-->
            <!--ONLY SL
            <dxschi:ViewContentStyleSelectorProperties GroupByNone="{StaticResource WeekViewThemeKey_WeekViewGroupByNone}"
               GroupByDate="{StaticResource WeekViewThemeKey_WeekViewGroupByDate}"
               GroupByResource="{StaticResource WeekViewThemeKey_WeekViewGroupByResource}" />
            ONLY SL-->
        </dxschi:WeekViewContentStyleSelector.Properties>
    </dxschi:WeekViewContentStyleSelector>

    <dxschi:TimelineViewContentStyleSelector x:Key="{dxscht:TimelineViewThemeKey ResourceKey=ContentStyleSelector}">
        <dxschi:TimelineViewContentStyleSelector.Properties>
            <!--BEGIN WPF-->
            <dxschi:ViewContentStyleSelectorProperties GroupByDate="{dxscht:TimelineViewThemeKey ResourceKey=TimelineViewGroupByDate}"
                                                       GroupByNone="{dxscht:TimelineViewThemeKey ResourceKey=TimelineViewGroupByNone}"
                                                       GroupByResource="{dxscht:TimelineViewThemeKey ResourceKey=TimelineViewGroupByDate}"/>
            <!--END WPF-->
            <!--ONLY SL
            <dxschi:ViewContentStyleSelectorProperties GroupByNone="{StaticResource TimelineViewThemeKey_TimelineViewGroupByNone}"
               GroupByDate="{StaticResource TimelineViewThemeKey_TimelineViewGroupByDate}"
               GroupByResource="{StaticResource TimelineViewThemeKey_TimelineViewGroupByDate}"/>
            ONLY SL-->
        </dxschi:TimelineViewContentStyleSelector.Properties>
    </dxschi:TimelineViewContentStyleSelector>

    <dxschi:GanttViewContentStyleSelector x:Key="{dxscht:GanttViewThemeKey ResourceKey=ContentStyleSelector}">
        <dxschi:GanttViewContentStyleSelector.Properties>
            <!--BEGIN WPF-->
            <dxschi:ViewContentStyleSelectorProperties GroupByDate="{dxscht:GanttViewThemeKey ResourceKey=GanttViewGroupByDate}"
                                                       GroupByNone="{dxscht:TimelineViewThemeKey ResourceKey=TimelineViewGroupByNone}"
                                                       GroupByResource="{dxscht:GanttViewThemeKey ResourceKey=GanttViewGroupByDate}"/>
            <!--END WPF-->
            <!--ONLY SL
            <dxschi:ViewContentStyleSelectorProperties GroupByNone="{StaticResource TimelineViewThemeKey_TimelineViewGroupByNone}"
                   GroupByDate="{StaticResource GanttViewThemeKey_GanttViewGroupByDate}"
                   GroupByResource="{StaticResource GanttViewThemeKey_GanttViewGroupByDate}" />
            ONLY SL-->
        </dxschi:GanttViewContentStyleSelector.Properties>
    </dxschi:GanttViewContentStyleSelector>

</ResourceDictionary>