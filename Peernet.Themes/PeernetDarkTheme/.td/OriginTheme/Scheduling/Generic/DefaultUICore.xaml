<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:dxschc="http://schemas.devexpress.com/winfx/2008/xaml/scheduling" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:cs="colorscheme_ignorable_uri" mc:Ignorable="cs">
    <SolidColorBrush x:Key="BrushSet.StatusBusy" Color="#FF4A87E2" />

<SolidColorBrush x:Key="BrushSet.StatusFree" Color="White" />

<SolidColorBrush x:Key="BrushSet.StatusOutOfOffice" Color="#FFD95353" />

<LinearGradientBrush x:Key="BrushSet.StatusTentative" MappingMode="Absolute" StartPoint="0,0" EndPoint="4,4" SpreadMethod="Repeat"><GradientStop Offset="0" Color="#FF4A87E2" cs:id="GradientStop_0000" /><GradientStop Offset="0.5" Color="#FF4A87E2" cs:id="GradientStop_0001" /><GradientStop Offset="0.5" Color="White" cs:id="GradientStop_0002" /><GradientStop Offset="1" Color="White" cs:id="GradientStop_0003" /></LinearGradientBrush>

<DrawingBrush x:Key="BrushSet.StatusWorkingElsewhere" Viewport="0,0,4,4" ViewportUnits="Absolute" TileMode="Tile"><DrawingBrush.Drawing><DrawingGroup><GeometryDrawing Brush="White" cs:id="GeometryDrawing_0000"><GeometryDrawing.Geometry><GeometryGroup FillRule="Nonzero"><RectangleGeometry Rect="0,0,80,80" /></GeometryGroup></GeometryDrawing.Geometry></GeometryDrawing><GeometryDrawing Brush="#FF937BD1" cs:id="GeometryDrawing_0001"><GeometryDrawing.Geometry><GeometryGroup FillRule="Nonzero"><RectangleGeometry Rect="0,0,20,20" /><RectangleGeometry Rect="40,40,20,20" /></GeometryGroup></GeometryDrawing.Geometry></GeometryDrawing></DrawingGroup></DrawingBrush.Drawing></DrawingBrush>

<dxschc:BrightnessColorTransform x:Key="BrushSet.TimeRegion.Color1.Transform.Light" Brightness="-0.4" OverrideA="#59" />

<dxschc:BrightnessColorTransform x:Key="BrushSet.TimeRegion.Color2.Transform.Light" Brightness="0.6" OverrideA="#40" />

<dxschc:BrightnessColorTransform x:Key="BrushSet.TimeRegion.Color1.Transform.Dark" Brightness="0.6" OverrideA="#59" />

<dxschc:BrightnessColorTransform x:Key="BrushSet.TimeRegion.Color2.Transform.Dark" Brightness="-0.4" OverrideA="#40" />

<!--ScheduleView--><SolidColorBrush x:Key="ScheuleView.TimeScale.TickBrush" Color="DarkGray" />

<SolidColorBrush x:Key="ScheuleView.TimeScale.BorderBrush" Color="Black" />

<sys:Boolean x:Key="ViewSelectorShowBackground">False</sys:Boolean>

</ResourceDictionary>