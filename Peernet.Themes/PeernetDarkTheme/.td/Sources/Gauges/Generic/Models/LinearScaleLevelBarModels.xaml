                                    <GradientStop Color="#FFD8D9E1"
                                                  Offset="1"
                                                  cs:id="GradientStop_0005"/>
                                </LinearGradientBrush>
                            </Rectangle.Fill>
                        </Rectangle>
                        <Ellipse Fill="#FFD8D8E1"
                                 Height="16"
                                 Margin="3,0,3,3"
                                 StrokeThickness="0"
                                 VerticalAlignment="Bottom"
                                 Width="16"
                                 cs:id="Ellipse_0003"/>
