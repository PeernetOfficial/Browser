    <Style x:Key="{dxt:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.DefaultLinearScaleLevelBarBackgroundControl}" TargetType="dxga:DefaultLinearScaleLevelBarBackgroundControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dxga:DefaultLinearScaleLevelBarBackgroundControl">
                    <Grid Margin="0,-12" RenderTransformOrigin="0.5, 1">
                        <Rectangle Fill="#FFB0B0B0"
                                   HorizontalAlignment="Center"
                                   RadiusX="5"
                                   RadiusY="5"
                                   StrokeThickness="0"
                                   Width="10"
                                   cs:id="Rectangle_0000"/>
                        <Rectangle Fill="#FFF7F7F7"
                                   HorizontalAlignment="Center"
                                   Margin="0,1"
                                   RadiusX="4"
                                   RadiusY="4"
                                   StrokeThickness="0"
                                   Width="8"
                                   cs:id="Rectangle_0001"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

