                                                  Offset="0"
                                                  cs:id="GradientStop_0000"/>
                                    <GradientStop Color="#FF88889D"
                                                  Offset="1"
                                                  cs:id="GradientStop_0001"/>
                                </LinearGradientBrush>
                            </Rectangle.Fill>
                        </Rectangle>
                        <Ellipse Fill="#FF898A9E"
                                 Height="20"
                                 Margin="1,0,1,1"
                                 StrokeThickness="0"
                                 VerticalAlignment="Bottom"
                                 Width="20"
                                 cs:id="Ellipse_0001"/>
                        <Ellipse Fill="#FFBFBFCE"
                                 Height="18"
                                 Margin="2"
                                 StrokeThickness="0"
                                 VerticalAlignment="Bottom"
                                 Width="18"
                                 cs:id="Ellipse_0002"/>
                        <Rectangle Margin="1,2,1,6"
                                   RadiusX="5"
                                   RadiusY="5"
                                   StrokeThickness="0"
                                   Width="10">
                            <Rectangle.Fill>
                                <LinearGradientBrush EndPoint="0.5,1"
                                                     StartPoint="0.5,0"
                                                     cs:id="LinearGradientBrush_0001">
                                    <GradientStop Color="#FFD3D5DE"
                                                  Offset="0"
                                                  cs:id="GradientStop_0002"/>
                                    <GradientStop Color="#FFBFBFCE"
                                                  Offset="1"
                                                  cs:id="GradientStop_0003"/>
                                </LinearGradientBrush>
                            </Rectangle.Fill>
                        </Rectangle>
                        <Rectangle Margin="6,3,6,6"
                                   RadiusX="4"
                                   RadiusY="4"
                                   StrokeThickness="0"
                                   Width="8">
                            <Rectangle.Fill>
                                <LinearGradientBrush EndPoint="0.5,1"
                                                     StartPoint="0.5,0"
                                                     cs:id="LinearGradientBrush_0002">
