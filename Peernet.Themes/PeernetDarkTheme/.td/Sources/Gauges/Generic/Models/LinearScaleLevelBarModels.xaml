                    <Grid Margin="0,-12,0,-19"
                          RenderTransformOrigin="0.5, 1"
                          Width="22">
                        <Ellipse Fill="White"
                                 Height="22"
                                 Margin="0"
                                 StrokeThickness="0"
                                 VerticalAlignment="Bottom"
                                 Width="22"
                                 cs:id="Ellipse_0000"/>
                        <Rectangle Fill="White"
                                   Margin="0,0,0,5"
                                   RadiusX="7"
                                   RadiusY="7"
                                   StrokeThickness="0"
                                   Width="14"
                                   cs:id="Rectangle_0000"/>
                        <Rectangle Margin="0,1,0,5"
                                   RadiusX="6"
                                   RadiusY="6"
                                   StrokeThickness="0"
                                   Width="12">
