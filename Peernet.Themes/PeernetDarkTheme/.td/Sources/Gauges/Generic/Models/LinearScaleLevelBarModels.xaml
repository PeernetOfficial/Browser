    <Style x:Key="{dxt:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.RedThermometerLinearScaleLevelBarBackgroundControl}" TargetType="dxga:RedThermometerLinearScaleLevelBarBackgroundControl">
        <Setter Property="Template">
