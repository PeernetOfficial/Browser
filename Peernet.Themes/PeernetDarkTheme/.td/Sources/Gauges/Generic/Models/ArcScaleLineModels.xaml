    </Style>

    <Style x:Key="{dxt:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.ThemeableArcScaleLineControl}" TargetType="dxga:ThemeableArcScaleLineControl">
        <Setter Property="ThemeFill" Value="{DynamicResource {dxgat:GaugesBrushesThemeKey ResourceKey=ScaleElementsBrush}}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dxga:ThemeableArcScaleLineControl">
                    <Rectangle Fill="{Binding Path=ActualFill, RelativeSource={RelativeSource TemplatedParent}}" RenderTransformOrigin="0.5, 0.5"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>