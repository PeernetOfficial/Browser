<ResourceDictionary mc:Ignorable="cs" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:cs="colorscheme_ignorable_uri" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxsch="http://schemas.devexpress.com/winfx/2008/xaml/scheduler" xmlns:dxschi="http://schemas.devexpress.com/winfx/2008/xaml/scheduler/internal" xmlns:dxscht="http://schemas.devexpress.com/winfx/2008/xaml/scheduler/themekeys" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxcore_unique_xmlns="http://schemas.devexpress.com/winfx/2008/xaml/core">

    

    <Style x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=HorizontalResourceHeaderStyle}" TargetType="{x:Type dxschi:VisualResourceHeader}">
        <Setter Property="dxcore_unique_xmlns:BlendHelper2.StyleInfo" Value="Scheduler\Generic\BaseView\HorizontalResourceHeader.xaml;14;6" /><Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type dxschi:VisualResourceHeader}">
                    <Grid dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Scheduler\Generic\BaseView\HorizontalResourceHeader.xaml;18;22">
                        <dxschi:HeaderControl Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" dxsch:SchedulerControl.HitTestType="ResourceHeader" dxschi:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschi:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}" HorizontalAlignment="Stretch" IsHitTestVisible="True" ResourceColor="{Binding Content.Brushes.ResourceHeader, RelativeSource={RelativeSource TemplatedParent}}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Scheduler\Generic\BaseView\HorizontalResourceHeader.xaml;19;26" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Grid HorizontalAlignment="Center" VerticalAlignment="Center" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Scheduler\Generic\BaseView\HorizontalResourceHeader.xaml;33;22">
                        <TextBlock Foreground="{DynamicResource {dxscht:SchedulerViewThemeKey ResourceKey=HeaderForegroundColor}}" HorizontalAlignment="Center" Text="{Binding ResourceCaption}" VerticalAlignment="Center" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Scheduler\Generic\BaseView\HorizontalResourceHeader.xaml;34;26">
                            
                            <TextBlock.TextTrimming>
                                <TextTrimming>CharacterEllipsis</TextTrimming>
                            </TextBlock.TextTrimming>
                            
                        </TextBlock>
                    </Grid>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>