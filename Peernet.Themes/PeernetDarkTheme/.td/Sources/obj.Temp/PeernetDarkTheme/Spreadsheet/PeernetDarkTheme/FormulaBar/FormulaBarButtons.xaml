    <Style x:Key="{dxspst:SpreadsheetThemeKey ResourceKey=FormulaBarCancelButtonStyle}" TargetType="{x:Type Button}">
        <Setter Property="dxcore_unique_xmlns:BlendHelper2.StyleInfo" Value="Spreadsheet\Office2019Black\FormulaBar\FormulaBarButtons.xaml;23;6" /><Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid x:Name="Root" MinHeight="22" MinWidth="22" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Spreadsheet\Office2019Black\FormulaBar\FormulaBarButtons.xaml;27;22">
                        <dx:DXBorder x:Name="IsDefault" Background="Transparent" cs:id="Border_0000" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Spreadsheet\Office2019Black\FormulaBar\FormulaBarButtons.xaml;30;26" />
