                <Grid dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Controls\Wizard\Generic\WizardControl.xaml;97;34">
                  <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition />
                    <RowDefinition Height="Auto" />
                  </Grid.RowDefinitions>
                  <StackPanel Margin="16,8,4,4" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="Controls\Wizard\Generic\WizardControl.xaml;103;38">
