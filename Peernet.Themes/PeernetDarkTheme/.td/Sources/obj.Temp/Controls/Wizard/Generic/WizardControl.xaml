  <dxco:WizardStringIdConverter x:Key="WizardStringIdConverter" />
  <dxmvvm:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
  <DataTemplate x:Key="{dxcot:WizardThemeKey ResourceKey=FooterTemplate}">
