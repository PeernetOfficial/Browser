  <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Controls.WelcomeWizardPage}" BasedOn="{StaticResource WizardPageBase}" TargetType="{x:Type dxco:WelcomeWizardPage}" />
  <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Controls.CompletionWizardPage}" BasedOn="{StaticResource WizardPageBase}" TargetType="{x:Type dxco:CompletionWizardPage}" />
  <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Controls.WizardFooterSeparator}" BasedOn="{StaticResource {dxt:DefaultStyleThemeKey FullName=System.Windows.Controls.Separator}}" TargetType="dxco:WizardFooterSeparator">
