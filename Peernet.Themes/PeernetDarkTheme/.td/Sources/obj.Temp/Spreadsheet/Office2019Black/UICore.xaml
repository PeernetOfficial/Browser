  <Color x:Key="UIColor.Spreadsheet.FormulaBar.Foreground.Ok">$Custom.Green</Color>
  <Color x:Key="UIColor.Spreadsheet.FormulaBar.Foreground.Function">$Custom.Blue</Color>
  
  <SolidColorBrush x:Key="{dxspst:SpreadsheetThemeKey ResourceKey=GroupControlBackgroundBrush}" Color="$Window.Background" />
  <SolidColorBrush x:Key="{dxspst:SpreadsheetThemeKey ResourceKey=GroupLineBrush}" Color="$Border" />
  <SolidColorBrush x:Key="{dxspst:SpreadsheetThemeKey ResourceKey=GroupDotBrush}" Color="$ControlLight.Foreground" />
  
  <Color x:Key="UIColor.Spreadsheet.ReferenceEdit.Foreground">$ControlLight.Foreground</Color>
  <Color x:Key="UIColor.Spreadsheet.ReferenceEdit.HoverForeground">$ControlLight.Foreground</Color>
