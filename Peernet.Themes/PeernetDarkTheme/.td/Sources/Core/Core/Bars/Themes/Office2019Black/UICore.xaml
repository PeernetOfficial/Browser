<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
                    xmlns:dxbt="http://schemas.devexpress.com/winfx/2008/xaml/bars/themekeys"
                    xmlns:dxi="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <dxt:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.Office2019Black.v22.2;component/Core/Core/Bars/Themes/Office2019Black/UICore_Base.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <!-- Font Settings -->

    <dxb:FontSettings x:Key="{dxbt:BarItemFontThemeKey ResourceKey=BarSplitButtonItemTextStyleInTextSplitter}"
                      Checked="$Foreground"
                      Hover="$Foreground"
                      HoverChecked="$Foreground"
                      Normal="$Foreground"
                      Pressed="$Foreground"/>

    <dxb:FontSettings x:Key="{dxbt:BarItemFontThemeKey ResourceKey=BarSubItemTextStyleInTextSplitter}"
                      Checked="$Foreground"
                      Hover="$Foreground"
                      HoverChecked="$Foreground"
                      Normal="$Foreground"
                      Pressed="$Foreground"/>

    <dxb:FontSettings x:Key="{dxbt:BarItemFontThemeKey ResourceKey=BarButtonItemTextStyleInTextSplitter}"
                      Checked="$Foreground"
                      Hover="$Foreground"
                      HoverChecked="$Foreground"
                      Normal="$Foreground"
                      Pressed="$Foreground"/>

    <dxb:FontSettings x:Key="{dxbt:BarItemFontThemeKey ResourceKey=BarCheckItemTextStyleInTextSplitter}"
                      Checked="$Foreground"
                      Hover="$Foreground"
                      HoverChecked="$Foreground"
                      Normal="$Foreground"
                      Pressed="$Foreground"/>

    <dxb:FontSettings x:Key="{dxbt:BarItemFontThemeKey ResourceKey=BarItemFontSettings}"
                      Checked="$SelectionForeground"
                      Hover="$HoverForeground"
                      HoverChecked="$HoverForeground"
                      Normal="$Foreground"
                      Pressed="$SelectionForeground"/>

    <dxb:FontSettings x:Key="{dxbt:BarItemFontThemeKey ResourceKey=BarItemFontSettingsInMainMenu}"
                      Checked="$SelectionForeground"
                      Hover="$HoverForeground"
                      HoverChecked="$HoverForeground"
                      Normal="$Foreground"
                      Pressed="$SelectionForeground"/>

    <dxb:FontSettings x:Key="{dxbt:BarItemFontThemeKey ResourceKey=BarItemFontSettingsInMenu}"
                      Checked="$ControlLight.SelectionForeground"
                      Hover="$ControlLight.HoverForeground"
                      HoverChecked="$ControlLight.SelectionForeground"
                      Normal="$ControlLight.Foreground"
                      Pressed="$ControlLight.SelectionForeground"/>

    <dxb:FontSettings x:Key="{dxbt:BarItemFontThemeKey ResourceKey=BarItemFontSettingsInApplicationMenu}"
                      Checked="$ControlLight.SelectionForeground"
                      Hover="$ControlLight.HoverForeground"
                      HoverChecked="$ControlLight.SelectionForeground"
                      Normal="$ControlLight.Foreground"
                      Pressed="$ControlLight.SelectionForeground"/>

    <dxb:FontSettings x:Key="{dxbt:BarItemFontThemeKey ResourceKey=BarItemDescriptionStyleInApplicationMenu}"
                      Checked="$ControlLight.SelectionForeground"
                      Hover="$ControlLight.HoverForeground"
                      HoverChecked="$ControlLight.SelectionForeground"
                      Normal="$ControlLight.Foreground"
                      Pressed="$ControlLight.SelectionForeground"/>

    <dxb:FontSettings x:Key="{dxbt:BarItemFontThemeKey ResourceKey=BarItemFontSettingsInStatusBar}"
                      Checked="$Foreground"
                      Hover="$Foreground"
                      HoverChecked="$Foreground"
                      Normal="$Foreground"
                      Pressed="$Foreground"/>

    <dxb:FontSettings x:Key="{dxbt:BarItemFontThemeKey ResourceKey=BarItemFontSettingsInRibbonStatusBarLeft}"
                      Checked="$Foreground"
                      Hover="$Foreground"
                      HoverChecked="$Foreground"
                      Normal="$Foreground"
                      Pressed="$Foreground"/>

    <dxb:FontSettings x:Key="{dxbt:BarItemFontThemeKey ResourceKey=BarItemFontSettingsInRibbonStatusBarRight}"
                      Checked="$Foreground"
                      Hover="$Foreground"
                      HoverChecked="$Foreground"
                      Normal="$Foreground"
                      Pressed="$Foreground"/>

    <dxb:FontSettings x:Key="{dxbt:BarItemFontThemeKey ResourceKey=BarItemFontSettingsInRibbonPageGroup}"
                      Checked="$ControlLight.SelectionForeground"
                      Hover="$ControlLight.HoverForeground"
                      HoverChecked="$ControlLight.SelectionForeground"
                      Normal="$ControlLight.Foreground"
                      Pressed="$ControlLight.SelectionForeground"/>

    <dxb:FontSettings x:Key="{dxbt:BarItemFontThemeKey ResourceKey=BarItemFontSettingsInRibbonToolbar}"
                      Checked="$Backstage.Foreground"
                      Hover="$Backstage.Foreground"
                      HoverChecked="$Backstage.Foreground"
                      Normal="$Backstage.Foreground"
                      Pressed="$Backstage.Foreground"/>

    <dxb:FontSettings x:Key="{dxbt:BarItemFontThemeKey ResourceKey=BarItemFontSettingsInRibbonToolbarFooter}"
                      Checked="$Foreground"
                      Hover="$Foreground"
                      HoverChecked="$Foreground"
                      Normal="$Foreground"
                      Pressed="$Foreground"/>

    <dxb:FontSettings x:Key="{dxbt:BarItemFontThemeKey ResourceKey=BarItemFontSettingsInRibbonPageHeader}"
                      Checked="$Backstage.Foreground"
                      Hover="$Backstage.Foreground"
                      HoverChecked="$Backstage.Foreground"
                      Normal="$Backstage.Foreground"
                      Pressed="$Backstage.Foreground"/>

    <dxb:FontSettings x:Key="{dxbt:BarItemFontThemeKey ResourceKey=BarItemFontSettingsInButtonGroup}"
                      Checked="$ControlLight.SelectionForeground"
                      Hover="$ControlLight.HoverForeground"
                      HoverChecked="$ControlLight.SelectionForeground"
                      Normal="$ControlLight.Foreground"
                      Pressed="$ControlLight.SelectionForeground"/>

    <!-- Image Colorizer Settings -->

    <dxb:BarItemImageColorizerSettings x:Key="{dxbt:BarItemImageColorizerSettingsThemeKey ResourceKey=DefaultSettings}"
                                       Checked="$Foreground"
                                       Hover="$Foreground"
                                       Normal="$Foreground"
                                       Pressed="$Foreground"/>

    <dxb:BarItemImageColorizerSettings x:Key="{dxbt:BarItemImageColorizerSettingsThemeKey ResourceKey=SettingsInButtonGroup}"
                                       Checked="$ControlLight.SelectionForeground"
                                       Hover="$ControlLight.HoverForeground"
                                       Normal="$ControlLight.Foreground"
                                       Pressed="$ControlLight.SelectionForeground"/>

    <dxb:BarItemImageColorizerSettings x:Key="{dxbt:BarItemImageColorizerSettingsThemeKey ResourceKey=SettingsInMainMenu}"
                                       Checked="$Foreground"
                                       Hover="$Foreground"
                                       Normal="$Foreground"
                                       Pressed="$Foreground"/>

    <dxb:BarItemImageColorizerSettings x:Key="{dxbt:BarItemImageColorizerSettingsThemeKey ResourceKey=SettingsInMenu}"
                                       Checked="$ControlLight.SelectionForeground"
                                       Hover="$ControlLight.HoverForeground"
                                       Normal="$ControlLight.Foreground"
                                       Pressed="$ControlLight.SelectionForeground"/>

    <dxb:BarItemImageColorizerSettings x:Key="{dxbt:BarItemImageColorizerSettingsThemeKey ResourceKey=SettingsInApplicationMenu}"
                                       Checked="$ControlLight.SelectionForeground"
                                       Hover="$ControlLight.HoverForeground"
                                       Normal="$ControlLight.Foreground"
                                       Pressed="$ControlLight.SelectionForeground"/>

    <dxb:BarItemImageColorizerSettings x:Key="{dxbt:BarItemImageColorizerSettingsThemeKey ResourceKey=SettingsInRibbonPageGroup}"
                                       Checked="$ControlLight.SelectionForeground"
                                       Hover="$ControlLight.HoverForeground"
                                       Normal="$ControlLight.Foreground"
                                       Pressed="$ControlLight.SelectionForeground"/>

    <dxb:BarItemImageColorizerSettings x:Key="{dxbt:BarItemImageColorizerSettingsThemeKey ResourceKey=SettingsInRibbonStatusBarLeft}"
                                       Checked="$Foreground"
                                       Hover="$Foreground"
                                       Normal="$Foreground"
                                       Pressed="$Foreground"/>

    <dxb:BarItemImageColorizerSettings x:Key="{dxbt:BarItemImageColorizerSettingsThemeKey ResourceKey=SettingsInRibbonStatusBarRight}"
                                       Checked="$Foreground"
                                       Hover="$Foreground"
                                       Normal="$Foreground"
                                       Pressed="$Foreground"/>

    <dxb:BarItemImageColorizerSettings x:Key="{dxbt:BarItemImageColorizerSettingsThemeKey ResourceKey=SettingsInRibbonToolbar}" Normal="$Backstage.Foreground"/>

    <dxb:BarItemImageColorizerSettings x:Key="{dxbt:BarItemImageColorizerSettingsThemeKey ResourceKey=SettingsInRibbonToolbarFooter}"
                                       Checked="$Foreground"
                                       Hover="$Foreground"
                                       Normal="$Foreground"
                                       Pressed="$Foreground"/>

    <dxb:BarItemImageColorizerSettings x:Key="{dxbt:BarItemImageColorizerSettingsThemeKey ResourceKey=SettingsInRibbonPageHeader}" Normal="$Backstage.Foreground"/>

    <dxb:BarItemImageColorizerSettings x:Key="{dxbt:BarItemImageColorizerSettingsThemeKey ResourceKey=SettingsInStatusBar}"
                                       Checked="$Foreground"
                                       Hover="$Foreground"
                                       Normal="$Foreground"
                                       Pressed="$Foreground"/>

    <!-- Colorizer -->

    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=NormalImageColorizeColor}">$Backstage.Foreground</Color>

    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=HoverImageColorizeColor}">$Backstage.Foreground</Color>

    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=PressedImageColorizeColor}">$Backstage.Foreground</Color>

    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=DisabledImageColorizeColor}">$Backstage.Foreground</Color>

</ResourceDictionary>