    <!-- Thickness -->

    <dxi:RenderStylesCollection x:Key="{dxbt:BarItemRenderThemeKey ResourceKey=TemplateInApplicationMenuStyles}"></dxi:RenderStylesCollection>

    <dxi:RenderStylesCollection x:Key="{dxbt:BarItemRenderThemeKey ResourceKey=SplitTemplateInApplicationMenuStyles}" BasedOn="{StaticResource {dxbt:BarItemRenderThemeKey ResourceKey=TemplateInApplicationMenuStyles}}"></dxi:RenderStylesCollection>

    <dxi:RenderStylesCollection x:Key="{dxbt:BarItemRenderThemeKey ResourceKey=EditTemplateInApplicationMenuStyles}" BasedOn="{StaticResource {dxbt:BarItemRenderThemeKey ResourceKey=TemplateInApplicationMenuStyles}}"></dxi:RenderStylesCollection>

</ResourceDictionary>