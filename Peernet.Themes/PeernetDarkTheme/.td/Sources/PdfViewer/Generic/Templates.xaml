                        Margin="0,0,16,0"
                        Orientation="Horizontal"
                        Tag="1"
                        Visibility="{Binding ShowTotalPageCount, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BoolToVisibilityConverter}}">
