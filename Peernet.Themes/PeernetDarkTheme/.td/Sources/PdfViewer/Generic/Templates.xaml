    <ControlTemplate x:Key="{dxpdft:PdfViewerThemeKey ResourceKey=PdfBarContainerControlTemplate}" TargetType="{x:Type dxb:BarContainerControl}">
        <ItemsPresenter x:Name="PART_ItemsPresenter" Margin="{TemplateBinding Padding}"/>
