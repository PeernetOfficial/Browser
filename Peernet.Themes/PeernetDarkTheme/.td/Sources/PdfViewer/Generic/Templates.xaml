        <Border Background="Transparent" cs:id="Border_0000">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <Image Height="16"
                       Margin="0,0,7,0"
                       Source="{dxpdf:PdfResource Images/Open_16x16.png}"
