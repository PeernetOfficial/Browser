                <ContentPresenter Grid.Column="1" HorizontalAlignment="Left">
                    <TextBlock.Foreground>
                        <SolidColorBrush Color="{DynamicResource {dxpdft:PdfViewerThemeKey ResourceKey=StartScreenOpenDocumentForegroundColor}}"/>
                    </TextBlock.Foreground>
