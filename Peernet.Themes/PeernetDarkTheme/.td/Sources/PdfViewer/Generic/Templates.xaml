                    <TextBlock.Foreground>
                        <SolidColorBrush Color="{DynamicResource {dxpdft:PdfViewerThemeKey ResourceKey=StartScreenDocumentLinkForegroundColor}}"/>
                    </TextBlock.Foreground>
